<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.102-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.ViewReportController">
   <children>
      <Button fx:id="waterSourceButton" layoutX="131.0" layoutY="163.0"
              mnemonicParsing="false" onAction="#handleViewWaterSourceReport"
              prefHeight="145.0" prefWidth="135.0" text="Water Source Report"
              wrapText="true"/>
      <Button fx:id="waterQualityButton" layoutX="329.0" layoutY="163.0"
              mnemonicParsing="false" prefHeight="145.0" prefWidth="135.0"
              text="Water  Quality Report" wrapText="true"/>
      <Label layoutX="131.0" layoutY="56.0" text="Pick a Report to View">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#handleBack" text="Back" />
   </children>
</AnchorPane>
